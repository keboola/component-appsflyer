{  
	"type":"object",
	"title":"AppsFlyer",
	"required":[  
		 "#api_token",
		 "reports"
	],
	"properties":{  
		 "#api_token":{  
				"type":"string",
				"title":"API token",
				"format":"password",
				"description":"",
				"propertyOrder":1
		 },
		 "reports":{  
				"type":"array",
				"items":{  
					 "type":"object",
					 "title":"Object",
					 "properties":{  
							"name":{  
								 "type":"string",
								 "title":"Report Name",
								 "propertyOrder":1
							},
							"Applications IDs":{  
								 "type":"string",
								 "title":"Applications IDs",
								 "default":"",
								 "propertyOrder":2
							},
"from_dt":{  
								 "type":"string",
								 "title":"Start Date",
								 "default":"",
								 "propertyOrder":3
							},
"to_dt":{  
								 "type":"string",
								 "title":"End Date",
								 "default":"",
								 "propertyOrder":3
							}
					 }
				},
				"title":"Reports",
				"format":"table",
				"default":[  
					 {  
							"name":"1",
							"Applications IDs":"",
"from_dt":"7 days ago",
"to_dt":"today"
					 }
				],
				"description":"Report name - report name (i.e. installs_report) \n Applications IDs - IDs of the apps you want to retrieve the report for.",
				"uniqueItems":true,
				"propertyOrder":2
		 }
	}
}